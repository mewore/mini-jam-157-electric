[display]

window/size/viewport_width=512
window/size/viewport_height=300
window/size/initial_position_type=3
window/size/window_width_override=1024
window/size/window_height_override=600
window/stretch/mode="viewport"

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
environment/defaults/default_clear_color=Color(0.427451, 0.521569, 0.647059, 1)
